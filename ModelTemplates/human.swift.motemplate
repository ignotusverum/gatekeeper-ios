import Synch
import CoreData

import PromiseKit

@objc(<$managedObjectClassName$>)
public class <$managedObjectClassName$>: _<$managedObjectClassName$> {

	// MARK: - Synchronization logic
	class func synchronize(withJSON JSON: [String : Any])-> Promise<<$managedObjectClassName$>?> {
		
		return Promise { fulfill, reject in
			
			Synch.changes(
				changes: JSON,
				inEntityNamed: <$managedObjectClassName$>.entityName(),
				dataStack: dataStack) { error in
					
					if let error = error {
						reject(error)
					}
					else {
						fulfill(nil)
					}
				}
			)
		}
	}
}
